{
  "name": "@dimensi/changeset-cli",
  "version": "1.1.0",
  "description": "CLI tool for managing changeset files",
  "main": "dist/src/index.js",
  "type": "module",
  "bin": {
    "changeset": "dist/src/index.js"
  },
  "scripts": {
    "build": "swc src -d dist",
    "dev": "swc src -d dist --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "changeset",
    "changelog",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.2",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3"
  }
}
